{
    "name": "sctr/greenrope-api-client",
    "description": "Client for greenrope API",
    "type": "library",
    "require": {
        "php": "^7",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer": "^1.13"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sctr\\Greenrope\\Api\\": "src"
        }
    },
    "autoload-dev":          {
        "psr-4": {
            "Sctr\\Greenrope\\Api\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "squizlabs/php_codesniffer": "^3.3"
    }
}
